#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap@16.3.4_ts-node@10.9.1_typescript@5.0.2/node_modules/tap/node_modules/loose-envify/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap@16.3.4_ts-node@10.9.1_typescript@5.0.2/node_modules/tap/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap@16.3.4_ts-node@10.9.1_typescript@5.0.2/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap@16.3.4_ts-node@10.9.1_typescript@5.0.2/node_modules/tap/node_modules/loose-envify/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap@16.3.4_ts-node@10.9.1_typescript@5.0.2/node_modules/tap/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap@16.3.4_ts-node@10.9.1_typescript@5.0.2/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../loose-envify/cli.js" "$@"
fi
