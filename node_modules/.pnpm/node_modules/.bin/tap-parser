#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap-parser@11.0.2/node_modules/tap-parser/bin/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap-parser@11.0.2/node_modules/tap-parser/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap-parser@11.0.2/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap-parser@11.0.2/node_modules/tap-parser/bin/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap-parser@11.0.2/node_modules/tap-parser/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/tap-parser@11.0.2/node_modules:/home/chhayya/Documents/work-code-example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tap-parser/bin/cmd.js" "$@"
else
  exec node  "$basedir/../tap-parser/bin/cmd.js" "$@"
fi
